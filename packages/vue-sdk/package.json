{
  "name": "@pantheon-systems/pcc-vue-sdk",
  "author": "@pantheon-systems",
  "description": "Pantheon Content Publisher Vue SDK",
  "version": "3.13.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js",
      "import": "./dist/components/index.mjs"
    },
    "./components/style.css": "./dist/components/index.css",
    "./nuxt": {
      "types": "./dist/platforms/nuxt.d.ts",
      "require": "./dist/platforms/nuxt.cjs",
      "import": "./dist/platforms/nuxt.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "nuxt": [
        "./dist/platforms/nuxt.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "tsup",
    "lint": "eslint . && prettier --check . && tsc --noEmit",
    "lint:fix": "eslint . --fix && prettier --write ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^20.19.13",
    "esbuild-plugin-globals": "^0.2.0",
    "eslint": "^8.57.1",
    "eslint-config-pcc-custom": "workspace:*",
    "h3": "^1.15.4",
    "sass": "^1.92.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "unplugin-vue": "^4.5.2",
    "vite": "^6.3.6",
    "vue": "3.4.27",
    "vue-3": "npm:vue@^3.4.27"
  },
  "peerDependencies": {
    "h3": "^1.8.2",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "h3": {
      "optional": true
    }
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@pantheon-systems/pcc-sdk-core": "workspace:*",
    "@vue/apollo-composable": "4.0.0-beta.12",
    "floating-vue": "^5.2.2",
    "graphql": "^16.11.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "html-url-attributes": "^3.0.1",
    "query-string": "^8.2.0",
    "rehype-raw": "^6.1.1",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vue-demi": "^0.14.10"
  },
  "sideEffects": false
}
