{
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ES2016",
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true
  }
}
